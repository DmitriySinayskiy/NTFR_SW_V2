  /* USER CODE BEGIN TIM1_TRG_COM_TIM11_IRQn 1 */

  /* USER CODE END TIM1_TRG_COM_TIM11_IRQn 1 */
}

/**
  * @brief This function handles TIM2 global interrupt.
  */
void TIM2_IRQHandler(void)
{
  /* USER CODE BEGIN TIM2_IRQn 0 */
	gl_counter_1000hz_tim2++;

	if(gl_counter_1000hz_tim2 % 50 == 0)
	{
		gl_int_5hz_tim2 = 1;
		gl_int_5hz_tim2_for_pic = 1;
	}

	if(gl_counter_1000hz_tim2 % 50 == 0 && gl_led_pwm_state == LED_PWM_CCR_CHANGEBALE )
	{
		ch3_counter += 3;
		if(ch3_counter < 250)
		{
			CH3_DC = ch3_counter;
			TIM2->CCR3 = CH3_DC;
		}
		else if(ch3_counter >= 250)
		{
			TIM2->CCR3 = CH3_DC;
			CH3_DC -= 3;
			if(CH3_DC <=0)
			{
				ch3_counter = 0;
				gl_led_pwm_state = LED_PWM_CCR_STABLE;
			}

		}


	}
  /* USER CODE END TIM2_IRQn 0 */
  HAL_TIM_IRQHandler(&htim2);
  /* USER CODE BEGIN TIM2_IRQn 1 */

  /* USER CODE END TIM2_IRQn 1 */
}

/**
  * @brief This function handles TIM4 global interrupt.
  */
void TIM4_IRQHandler(void)
{
  /* USER CODE BEGIN TIM4_IRQn 0 */


  /* USER CODE END TIM4_IRQn 0 */
  HAL_TIM_IRQHandler(&htim4);
  /* USER CODE BEGIN TIM4_IRQn 1 */


  /* USER CODE END TIM4_IRQn 1 */
}

