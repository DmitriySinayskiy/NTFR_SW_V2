typedef struct
{
	uint8_t main_probe;
	uint8_t sub_probe;

	uint8_t main_settings;
	uint8_t sub_settings_sleep;
	uint8_t sub_settings_lang;

	uint8_t main_info;
	uint8_t sub_info;
} menu_page_flag;

extern menu_page_flag menu_flag;
void main_menu_probe_action()

#endif /* MAIN_MENU_PROBE_H_ */
